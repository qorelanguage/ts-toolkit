{
  "name": "@qoretechnologies/qorus-toolkit",
  "version": "0.2.2",
  "description": "Utility library to interact with Qorus Integration Engine",
  "keywords": [
    "qoretechnologies",
    "qorus-authenticator"
  ],
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "exports": {
    "node": {
      "import": "./feature-node.mjs",
      "require": "./feature-node.cjs"
    },
    "browser": {
      "import": "./feature.mjs"
    }
  },
  "jest": {
    "preset": "ts-jest",
    "coverageReporters": [
      "json-summary",
      "text",
      "lcov"
    ]
  },
  "pre-push": [
    "lint",
    "test",
    "updatePullRequestVersion"
  ],
  "scripts": {
    "test": "./node_modules/.bin/jest --json --outputFile=tests.json",
    "test:coverage": "npm test -- --coverage",
    "test:badges": "npm run test:coverage  && jest-coverage-badges",
    "build": "npx tsc",
    "build:test": "npx tsc --noEmit",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint . --ext .ts",
    "docs": "npm run css & typedoc && npm run updateDocs && node ./src/examples/addExamplesToDocs.js && cpx ./coverage/**/* ./docs/coverage",
    "docs:watch": "npm run css & typedoc --watch",
    "updateDocs": "chmod +x updateDocs.sh && ./updateDocs.sh",
    "css": "./node_modules/.bin/sass src/typedoc/index.scss src/typedoc/index.css",
    "css:watch": "./node_modules/.bin/sass --watch src/typedoc/index.scss src/typedoc/index.css",
    "prepublishOnly": "npm test && npm run lint",
    "updatePullRequestVersion": "chmod +x prePush.sh && ./prePush.sh",
    "storybook": "start-storybook -p 6006 --no-dll",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qoretechnologies/qorus-toolkit.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/qoretechnologies/qorus-toolkit/issues"
  },
  "homepage": "https://github.com/qoretechnologies/qorus-toolkit#readme",
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@qoretechnologies/reqore": "^0.25.5",
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-interactions": "^6.5.13",
    "@storybook/addon-jest": "^6.5.13",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/addon-storysource": "^6.5.13",
    "@storybook/builder-webpack5": "^6.5.13",
    "@storybook/manager-webpack5": "^6.5.13",
    "@storybook/react": "^6.5.13",
    "@storybook/testing-library": "^0.0.13",
    "@tsconfig/node16": "^1.0.3",
    "@types/jest": "^28.1.7",
    "@types/node": "^18.7.11",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "axios-mock-adapter": "^1.21.2",
    "babel-loader": "^8.3.0",
    "chromatic": "^6.11.4",
    "dotenv": "^16.0.2",
    "eslint": "^8.22.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "pre-push": "^0.1.3",
    "prettier": "^2.7.1",
    "react": "18.2.0",
    "react-dom": "^18.2.0",
    "react-runkit": "^0.9.0",
    "react-syntax-highlighter": "^15.5.0",
    "sass": "^1.55.0",
    "storybook-addon-outline": "^1.4.2",
    "storybook-dark-mode": "^1.1.2",
    "ts-jest": "^28.0.8",
    "typedoc": "^0.23.10",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@typescript-eslint/parser": "^5.42.1",
    "async": "^3.2.4",
    "axios": "^0.27.2",
    "cpx": "^1.5.0",
    "cron-validator": "^1.3.1",
    "del-cli": "^5.0.0",
    "eslint-plugin-import": "^2.26.0",
    "get-all-files": "^4.1.0",
    "jest-coverage-badges": "^1.0.0",
    "jest-mock-axios": "^4.7.0-beta",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "ssl-root-cas": "^1.3.1",
    "supertest": "^6.2.4",
    "tracer": "^1.1.6"
  }
}
