{
  "name": "@qoretechnologies/qorus-toolkit",
  "version": "0.1.1",
  "description": "Utility library to interact with Qorus Integration Engine",
  "keywords": [
    "qoretechnologies",
    "qorus-authenticator"
  ],
  "type": "module",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "jest": {
    "preset": "ts-jest"
  },
  "pre-push": ["lint","test","updatePullRequestVersion"],
  "scripts": {
    "test": "jest",
    "build": "npx tsc",
    "build:test": "npx tsc --noEmit",
    "clean": "del-cli --force ./dist",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint . --ext .ts",
    "docs": "typedoc ./src/index.ts --out docs ./docs && npm run updateDocs",
    "updateDocs": "chmod +x updateDocs.sh && ./updateDocs.sh",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "updatePullRequestVersion": "chmod +x prePush.sh && ./prePush.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qoretechnologies/qorus-toolkit.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/qoretechnologies/qorus-toolkit/issues"
  },
  "homepage": "https://github.com/qoretechnologies/qorus-toolkit#readme",
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/jest": "^28.1.7",
    "@types/node": "^18.7.11",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "axios-mock-adapter": "^1.21.2",
    "eslint": "^8.22.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "pre-push": "^0.1.3",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.8",
    "typedoc": "^0.23.10",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "async": "^3.2.4",
    "axios": "^0.27.2",
    "del-cli": "^5.0.0",
    "dotenv": "^16.0.1",
    "lodash": "^4.17.21",
    "ssl-root-cas": "^1.3.1",
    "supertest": "^6.2.4",
    "validator": "^13.7.0",
    "winston": "^3.8.1"
  }
}
