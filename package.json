{
  "name": "qorus-toolkit-test",
  "version": "0.1.2",
  "description": "Utility library to interact with Qorus Integration Engine",
  "keywords": ["qoretechnologies", "qorus-authenticator"],
  "main": "dist/src/index.js",
  "typings": "dist/src/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "test": "jest",
    "build": "npx tsc",
    "clean": "del-cli --force ./dist",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qoretechnologies/qorus-toolkit.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/qoretechnologies/qorus-toolkit/issues"
  },
  "homepage": "https://github.com/qoretechnologies/qorus-toolkit#readme",
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/jest": "^28.1.7",
    "@types/node": "^18.7.11",
    "axios-mock-adapter": "^1.21.2",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.8",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.23.10",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "async": "^3.2.4",
    "axios": "^0.27.2",
    "del-cli": "^5.0.0",
    "lodash": "^4.17.21",
    "ssl-root-cas": "^1.3.1",
    "supertest": "^6.2.4",
    "validator": "^13.7.0",
    "winston": "^3.8.1"
  }
}
